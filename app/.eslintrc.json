{
    "extends": "eslint:recommended",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "env": {
        "browser": true
    },
    "rules": {
        "no-console": "off",
        "semi": [
            "error",
            "never"
        ],
        "quotes": ["error", "single"]
    },
    "globals": {
        "jQuery": false,
        "ver": true,
        "pwa": true,
        "useLang": true,
        "global": true
    }
}